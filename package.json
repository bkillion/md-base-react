{
    "name": "md-base-react",
    "version": "0.1.0",
    "description": "Master developer base React web framework",
    "author": "Brian Killion",
    "scripts": {
      "open:dist": "node ./node_modules/babel-cli/bin/babel-node config/dist-server.js",
      "open:dev": "node ./node_modules/babel-cli/bin/babel-node config/dev-server.js",
      "start": "npm run open:dev",
      "build": "node ./node_modules/babel-cli/bin/babel-node config/build.js && npm run open:dist",
      "clean-dist": "npm run remove-dist && mkdir dist",
      "remove-dist": "rimraf ./dist",
      "prebuild": "npm run clean-dist",
      "test": "jest --config ./config/jest.config.js",
      "debug": "node ./node_modules/babel-cli/bin/babel-node  --inspect-brk=30569 --nolazy ./tests.js | tap-spec"
    },
    "dependencies": {
      "axios": "^0.18.0",
      "es6-promise": "^4.2.4",
      "material-ui": "^0.20.0",
      "react": "^16.3.2",
      "react-dom": "^16.3.2",
      "react-redux": "^5.0.6",
      "react-router": "^4.1.2",
      "react-router-dom": "^4.2.0",
      "redux": "^3.7.2",
      "redux-logger": "^3.0.6",
      "redux-thunk": "^2.2.0"
    },
    "main": "./index.js",
    "license": "Public",
    "devDependencies": {
      "autoprefixer": "^8.2.0",
      "babel-cli": "^6.26.0",
      "babel-core": "^6.26.0",
      "babel-loader": "^7.1.1",
      "babel-plugin-react-transform": "^3.0.0",
      "babel-preset-env": "^1.6.1",
      "babel-preset-react": "^6.24.1",
      "babel-preset-stage-0": "^6.24.1",
      "browser-sync": "^2.23.6",
      "chalk": "^2.3.0",
      "connect-history-api-fallback": "^1.5.0",
      "css-loader": "^0.28.5",
      "deep-freeze": "0.0.1",
      "enzyme": "^3.3.0",
      "enzyme-adapter-react-16": "^1.1.1",
      "eslint": "^4.19.1",
      "eslint-plugin-react": "^7.6.1",
      "extract-text-webpack-plugin": "^3.0.0",
      "file-loader": "^1.1.6",
      "html-webpack-plugin": "^3.2.0",
      "jest": "^22.2.1",
      "node-babel": "^0.1.2",
      "node-sass": "^4.5.3",
      "postcss-loader": "^2.1.0",
      "react-hot-loader": "^4.0.1",
      "rimraf": "^2.6.2",
      "sass-loader": "^7.0.1",
      "style-loader": "^0.20.1",
      "tap-spec": "^4.1.1",
      "tape": "^4.8.0",
      "url-loader": "^1.0.1",
      "webpack": "^3.5.5",
      "webpack-dev-middleware": "^2.0.4",
      "webpack-dev-server": "^2.7.1",
      "webpack-hot-middleware": "^2.21.2",
      "webpack-md5-hash": "0.0.6"
    },
    "jest": {
      "moduleNameMapper": {
        "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/assets-xformer.js",
        "\\.(css)$": "<rootDir>/config/assets-xformer.js"
      }
    }
  }
  